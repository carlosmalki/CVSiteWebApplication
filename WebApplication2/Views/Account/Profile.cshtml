@model User; 

@{
    ViewData["Title"] = "Profile Picture Uploaded";
}

<h2>Your Profile Picture</h2>

@if (Model.ProfilePictureUrl != null)
{
    <img src="@Url.Content(Model.ProfilePictureUrl)" alt="Profile Picture" style="max-width: 300px; max-height: 300px;" />
}
else
{
    <p>No profile picture uploaded.</p>
}
