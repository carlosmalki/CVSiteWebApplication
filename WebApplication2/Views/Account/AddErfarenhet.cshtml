@model WebApplication2.Models.Erfarenhet

@{
    ViewData["Title"] = "Lägg till Erfarenhet";
}

<h2>Lägg till Erfarenhet</h2>

<form asp-action="AddErfarenhet" asp-controller="Account" method="post">
    <div class="form-group">
        <label for="Title">Erfarenhetens Titel</label>
        <input type="text" class="form-control" id="Title" name="Title" required>
    </div>
    <div class="form-group">
        <label for="Beskrivning">Beskrivning</label>
        <textarea class="form-control" id="Beskrivning" name="Beskrivning" required></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Lägg till</button>
</form>
