@model WebApplication2.Models.Utbildning

@{
    ViewData["Title"] = "Lägg till Utbildning";
}

<h2>Lägg till Utbildning</h2>

<form asp-action="AddUtbildning" asp-controller="Account" method="post">
    <div class="form-group">
        <label for="Namn">Utbildningens Namn</label>
        <input type="text" class="form-control" id="Namn" name="Namn" required>
    </div>
    <div class="form-group">
        <label for="Beskrivning">Beskrivning</label>
        <textarea class="form-control" id="Beskrivning" name="Beskrivning" required></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Lägg till</button>
</form>

